{
  "name": "changez",
  "version": "2.1.2",
  "description": "Yet another flexible changelog generator.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishonly": "npm run build",
