  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/commander": "^2.12.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/nunjucks": "^3.1.4",
    "@types/shelljs": "^0.8.8",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "source-map-support": "^0.5.19",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "commander": "^7.2.0",
    "find-package": "^1.0.0",
    "mkdirp": "^1.0.4",
